  },
  "homepage": "https://github.com/duck4nh/antigravity-kit#readme",
  "bugs": {
    "url": "https://github.com/duck4nh/antigravity-kit/issues"
  },
  "keywords": [
    "antigravity",
    "cli",
    "ai",
    "agent",
    "gemini",
    "skills"
  ],
  "author": "duck4nh",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "ora": "^8.2.0"
  }
